{
  "theme": {
    "file": "/ventoy/theme/vimix/theme.txt",
    "gfxmode": "max",
    "ventoy_color": "#FFFFFF",
    "fonts": [
      "/ventoy/theme/vimix/terminus-12.pf2",
      "/ventoy/theme/vimix/terminus-14.pf2",
      "/ventoy/theme/vimix/terminus-16.pf2",
      "/ventoy/theme/vimix/terminus-18.pf2"
    ]
  },
  "menu_alias": [
    {
      "image": "/iso/ubuntu-22.04-desktop-amd64.iso",
      "alias": "Ubuntu 22.04"
    },
    {
      "image": "/iso/kali-linux-2020.4-live-amd64.iso",
      "alias": "Kali Linux 2020.4"
    },
    {
      "image": "/iso/windows-10-21H2-x64-19044.iso",
      "alias": "Windows 10 21H2 November 2021"
    },
    {
      "image": "/iso/windows-11-22H2-x64-22621.iso",
      "alias": "Windows 11 22H2 September 2022"
    },
    {
      "image": "/iso/chromeos_15437.61.0_reven_recovery_stable-channel_mp-v2.img",
      "alias": "ChromeOS Flex 15437"
    },
    {
      "image": "/iso/netboot.xyz.iso",
      "alias": "netboot.xyz"
    }
  ],
  "menu_class": [
    {
      "key": "ubuntu",
      "class": "ubuntu"
    },
    {
      "key": "kali",
      "class": "kali"
    },
    {
      "key": "windows",
      "class": "windows"
    },
    {
      "key": "chromeos",
      "class": "brunch"
    },
    {
      "key": "netboot",
      "class": "driver"
    }
  ],
  "persistence": [
    {
      "image": "/iso/ubuntu-20.04.2.0-desktop-amd64.iso",
      "backend": ["/persistence/persistence_ext4_4GB_casper-rw.dat"]
    },
    {
      "image": "/iso/kali-linux-2020.4-live-amd64.iso",
      "backend": ["/persistence/persistence_ext4_4GB_persistence.dat"]
    }
  ]
}
