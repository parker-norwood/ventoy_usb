{
  "theme": {
    "file": "/ventoy/theme/vimix/theme.txt",
    "gfxmode": "max",
    "ventoy_color": "#FFFFFF",
    "fonts": [
      "/ventoy/theme/vimix/terminus-12.pf2",
      "/ventoy/theme/vimix/terminus-14.pf2",
      "/ventoy/theme/vimix/terminus-16.pf2",
      "/ventoy/theme/vimix/terminus-18.pf2"
    ]
  },
  "menu_alias": [
    {
      "image": "/iso/ubuntu-24.04-desktop-amd64.iso",
      "alias": "Ubuntu 24.04"
    },
    {
      "image": "/iso/Win11_23H2_English_x64v2.iso",
      "alias": "Windows 11 23H2"
    },
    {
      "image": "/iso/kali-linux-2024.1-live-amd64.iso",
      "alias": "Kali Linux 2024.1"
    }
  ],
  "menu_class": [
    {
      "key": "ubuntu",
      "class": "ubuntu"
    },
    {
      "key": "kali",
      "class": "kali"
    },
    {
      "key": "Win11",
      "class": "windows"
    },
    {
      "key": "chromeos",
      "class": "brunch"
    },
    {
      "key": "netboot",
      "class": "driver"
    }
  ],
  "persistence": [
    {
      "image": "/iso/ubuntu-24.04-desktop-amd64.iso",
      "backend": ["/persistence/persistence_ext4_4GB_casper-rw.dat"]
    },
    {
      "image": "/iso/kali-linux-2024.1-live-amd64.iso",
      "backend": ["/persistence/persistence_ext4_4GB_persistence.dat"]
    }
  ],
  "image_list": [
    "/iso/ubuntu-24.04-desktop-amd64.iso",
    "/iso/Win11_23H2_English_x64v2.iso",
    "/iso/kali-linux-2024.1-live-amd64.iso"
  ],
  "control": [
    { "VTOY_WIN11_BYPASS_CHECK": "1" },
    { "VTOY_WIN11_BYPASS_NRO": "1"}
  ]
}
